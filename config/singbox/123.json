{
  "dns": {
    "final": "dns_direct",
    "fake_ip": {
      "enabled": true,
      "ip_range": [
        "198.18.0.0/15",
        "fc00::/18"
      ]
    },
    "server": [
      {
        "tag": "dns_direct",
        "address": "https://120.53.53.53/dns-query",
        "detour": "direct"
      },
      {
        "tag": "dns_proxy",
        "address": "https://dns.google/dns-query",
        "address_resolver": "dns_direct",
        "strategy": "ipv4_only",
        "client_subnet": "1.0.1.0",
        "detour": "proxy"
      },
      {
        "tag": "dns_block",
        "address": "rcode://refused"
      },
      {
        "tag": "fakedns",
        "address": "fakeip"
      }
    ],
    "rule": [
      {
        "outbound": "proxy",
        "server": "dns_proxy"
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "server": "dns_direct"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "server": "fakedns",
        "disable_cache": true
      }
    ]
  },

  "route": {
    "rule_set": [
      {
        "tag": "geosite-geolocation-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-geolocation-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-!cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs",
        "download_detour": "proxy"
      }
    ],
    "rule": [
      {
        "ip_is_private": true,
        "action": "route",
        "outbound": "direct"
      },
      {
        "rule_set": [
          "geosite-geolocation-cn",
          "geoip-cn"
        ],
        "action": "route",
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "action": "route",
        "outbound": "proxy"
      }
    ],
    "final": "proxy",
    "auto_detect_interface": true
  }
}
